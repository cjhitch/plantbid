		"@types/jest": "^29.5.7",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"@typescript-eslint/eslint-plugin": "^6.7.5",
		"eslint": "^8.2.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-next": "13.5.4",
		"eslint-config-prettier": "^9.0.0",
